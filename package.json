{
  "name": "aerux",
  "version": "2.0.1",
  "description": "An easy way to use redux with react",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "npm run test",
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "babel src -d es",
    "build:cjs": "BABEL_ENV=commonjs babel src -d lib",
    "prelint": "npm run test",
    "lint": "eslint ./src && flow",
    "lint:fix": "eslint ./src --fix",
    "flow": "flow",
    "preversion": "npm run test",
    "coverage": "npm run test && cat ./coverage/lcov.info | coveralls",
    "publishOnly": "npm run build"
  },
  "files": [
    "lib",
    "es",
    "src",
    "flow-typed"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kagawagao/aerux.git"
  },
  "keywords": [
    "redux",
    "react-redux",
    "redux-actions"
  ],
  "author": {
    "name": "kagawagao",
    "email": "kingsongao1221@gmail.com",
    "url": "http://kingsongao.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kagawagao/aerux/issues"
  },
  "homepage": "https://github.com/kagawagao/aerux#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.93.0",
    "jest": "^24.1.0",
    "lodash": "^4.17.11",
    "react": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^16.8.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "lodash": "^4.17.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  }
}
